{
  "expo": {
    "name": "BioGO",
    "slug": "BioGO",
    "scheme": "biogo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/BioGO_logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/BioGO_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "package": "com.bioribas.biogo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/BioGO_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bioribas.biogo"
    },
    "web": {
      "favicon": "./assets/BioGO_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow BioGO to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ]
  }
}
